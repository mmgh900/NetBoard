<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="395.0" prefWidth="395.0"
            style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="controllers.ChatBoxController">
   <children>
      <VBox alignment="BOTTOM_CENTER" layoutX="128.0" layoutY="88.0" prefHeight="395.0" prefWidth="395.0"
            style="-fx-background-color: white;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ScrollPane hbarPolicy="NEVER" hvalue="1.0" vbarPolicy="NEVER" vvalue="1.0">
               <content>
                  <VBox fx:id="massages" alignment="CENTER">
                     <children>
                        <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT" prefWidth="395.0" spacing="5.0">
                           <children>
                              <Button id="smallButton" mnemonicParsing="false" text="hi">
                                 <HBox.margin>
                                    <Insets/>
                                 </HBox.margin>
                              </Button>
                              <Text fill="#8d8d8d" strokeType="OUTSIDE" strokeWidth="0.0" text="12:31"/>
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" nodeOrientation="RIGHT_TO_LEFT"
                              prefWidth="395.0" spacing="5.0">
                           <children>
                              <Button id="smallButton" mnemonicParsing="false"
                                      text="sdddddddddddddddddddddddddddddddddddddddddd" wrapText="true">
                                 <HBox.margin>
                                    <Insets/>
                                 </HBox.margin>
                              </Button>
                              <Text fill="#8d8d8d" strokeType="OUTSIDE" strokeWidth="0.0" text="12:31"/>
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
                           </padding>
                        </HBox>
                     </children>
                  </VBox>
               </content>
            </ScrollPane>
            <HBox prefHeight="36.0" prefWidth="355.0" spacing="10.0" style="-fx-background-color: BLACK;">
               <children>
                  <TextField id="chat" fx:id="textField" prefHeight="33.0" prefWidth="255.0" promptText="Text massage"
                             style="-fx-pref-height: 30;"/>
                  <Button id="smallButton" fx:id="send" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0"
                          text="SEND"/>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
